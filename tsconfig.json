{
	"compilerOptions": {
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"target": "es2022",
		"module": "commonjs",
		"moduleResolution": "node",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"incremental": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictBindCallApply": true,
		"skipLibCheck": true
	}
}